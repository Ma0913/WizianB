<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wizian.admission.wizianb.repository.ApplicationInfoRepository">

    <select id="findAplyInfoByMemIdAndCourseDiv" resultType="com.wizian.admission.wizianb.domain.ApplicationInfo">
        SELECT * FROM entry_apply_master
        WHERE mem_id = #{memId}
        AND course_div = #{courseDiv}
    </select>

</mapper>